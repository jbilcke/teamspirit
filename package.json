  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.story.{js,jsx,ts,tsx}",
      "!src/**/sportsdb.js",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/serviceWorker.js",
      "!src/fixtures/**/*.js"
    ]
  }
